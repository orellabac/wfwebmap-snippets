{
  "Adodc": {
    "prefix": "wm-adodatacontrol",
    "body": [
      "<wm-adodatacontrol id=\"${1:id}\" [model]=\"model.${1:id}\" class=\"${1:id}\">",
      "</wm-adodatacontrol>"
      ],
    "description": "Angular Data Control Control"
  },
