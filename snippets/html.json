  "Window": {
      "prefix": "wm-window",
      "body": [
        "<div *ngIf=\"model\">",
        "\t<wm-window [model]=\"model\" id=\"${1:id}\" class=\"${1:id}\">",
        "\t\t<ng-template let-model>",
            "\t\t\t<div class=\"${1:id}\">",
            "\t\t\t$0",
            "\t\t\t</div>",
        "\t\t</ng-template>",
        "\t</wm-window>",
        "</div>"
      ],
      "description": "Angular Window"
    }
